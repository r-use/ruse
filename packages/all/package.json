{
  "name": "@r-use/all",
  "version": "0.2.1",
  "packageManager": "pnpm@7.5.0",
  "description": "The next-gen react hooks library.",
  "keywords": [
    "react",
    "react-hooks",
    "react-utils",
    "react-use"
  ],
  "license": "MIT",
  "author": "Ray <nn_201312@163.com> (@so1ve)",
  "homepage": "https://github.com/r-use/ruse#readme",
  "bugs": {
    "url": "https://github.com/r-use/ruse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-use/ruse.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@r-use/use-breakpoints": "workspace:*",
    "@r-use/use-media-query": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
